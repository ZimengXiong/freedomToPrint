{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2021", "dom"],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "noEmit": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["./_generated", "./**/*.js"]
}
